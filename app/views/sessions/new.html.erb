<h1>Login</h1>
<%= form_for :login, url: '/login' do |f| %>
  <div>
    <%= label_tag :email %>
    <%= f.email_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
    <%= f.submit "Submit" %>

<%end%>